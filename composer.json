{
    "name": "anacona16/imagecrop-bundle",
    "type": "symfony-bundle",
    "description": "Image Crop for Symfony applications",
    "keywords": ["image", "crop"],
    "homepage": "https://github.com/anacona16/ImageCropBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ramiro Anacona Meneses",
            "email": "anacona16@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "liip/imagine-bundle": "^2.6",
        "symfony/filesystem": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/finder": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/framework-bundle": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/form": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/templating": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/translation": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/yaml": "^4.4.31 || ^5.3 || ^6.0",
        "vich/uploader-bundle": "^1.18",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.9",
        "symfony/phpunit-bridge": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/browser-kit": "^4.4.31 || ^5.3 || ^6.0",
        "symfony/css-selector": "^4.4.31 || ^5.3 || ^6.0"
    },
    "autoload": {
        "psr-4": { "Anacona16\\Bundle\\ImageCropBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Anacona16\\Bundle\\ImageCropBundle\\Tests\\": "Tests/" }
    }
}
